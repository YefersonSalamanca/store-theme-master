{
    "flex-layout.row#home1": {
        "props":{
            "fullWidth":true
        },
        "children": ["flex-layout.col#home1"]
      },

      "flex-layout.col#home1": {
          "props":{"width":"100%"},
          "children":[
            "list-context.image-list#demo",
            "__fold__",
            "flex-layout.row#bannerComplemento",
            "rich-text#shelf-title",
            "flex-layout.row#shelf",
            "flex-layout.row#setInfocart",
            "flex-layout.row#bannerInferior",
            "flex-layout.row#formulario",      
            "rich-text#link",
            "flex-layout.row#redessociales"
        ]

      },    
      "rich-text#home-1": {
        "props": {
          "text": "Este es el Home LiliPink"
        
        }
      },

      "list-context.image-list#demo": {
        "children": ["slider-layout#demo-images"],
        "props": {
          "height": 720,
          "images": [
            {
              "src": "assets/BannersMainSlider/sh1.jpg" , 
              "link": {
                "url":"/lili-pink/brasieres?O=OrderByTopSaleDESC"
              },
              "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
            },
            {
              "src": "assets/BannersMainSlider/sh2.jpg" , 
              "mobileImage": "https://w.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
            },
            {
              "src": "assets/BannersMainSlider/sh3.jpg" , 
              "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
            },
            {
              "src": "assets/BannersMainSlider/sh4.jpg" , 
              "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
            }
          ]
        }
      },
      "slider-layout#demo-images": {
        "props": {
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          },
          "infinite": true,
          "showNavigationArrows": "desktopOnly",
          "blockClass": "carousel"
        }
      },
      "info-card#homeHorizontal": {
        "props": {
          "id": "info-card-homeHorizontal",
          "isFullModeStyle": true,
           "imageUrl": "assets/BannerDeal.jpg",
           "blockClass": "info-card-homeHorizontal"
          
        }
      },
      "rich-text#shelf-title": {
        "props": {
          "text": "## Para ti",
          "blockClass": "shelfTitle"
        }
      },
      "flex-layout.row#shelf": {
        "children": ["list-context.product-list#demo1"]
      },
      "list-context.product-list#demo1": {
        "blocks": ["product-summary.shelf"],
        "children": ["slider-layout#demo-products"],
        "props": {
          "orderBy": "OrderByTopSaleDESC"
        }
      },
      "slider-layout#demo-products": {
        "props": {
          "itemsPerPage": {
            "desktop": 4,
            "tablet": 3,
            "phone": 1
          },
          "infinite": true,
          "fullWidth": false,
          "blockClass": "shelf"
        }
      },
    
    
      "info-card#home": {
        "props": {
          "id": "info-card-home",
          "isFullModeStyle": false,
          "textPosition": "left",
          "imageUrl": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
          "headline": "Clearance Sale",
          "callToActionText": "DISCOVER",
          "callToActionUrl": "/sale/d",
          "blockClass": "info-card-home",
          "textAlignment": "center"
        }
      },
      "flex-layout.row#bannerComplemento": {
        "props": {
          "verticalAlign": "middle",
          "horizontalAlign": "center"
        },
        "children": ["image#bannerSlim"]
      },
     
      
      "image#bannerSlim": {  
        "props": {  
          "src": "assets/BannerDeal.jpg"  
        }  
      },
      "flex-layout.row#setInfocart": {
        "props": {
           "verticalAlign": "middle",
          "horizontalAlign": "center"
        },
        "children": ["info-card#ic1", "info-card#ic2", "info-card#ic3"]
      },
      "info-card#ic1": {
        "props": {
          "id": "info-card-ic1",
          "isFullModeStyle":true,
           "imageUrl": "assets/infoHomeA.jpg",
           "callToActionText": "COMPRA AQUÍ",
          "callToActionUrl": "/sale/d",
          "blockClass": "info-card",
          "textAlignment": "center"
        }
      },
      "info-card#ic2": {
        "props": {
          "id": "info-card-ic2",
          "isFullModeStyle": true,
           "imageUrl": "assets/infoHome2.jpg",  
           "callToActionText": "COMPRA AQUÍ",
          "callToActionUrl": "/sale/d",
          "blockClass": "info-card",
          "textAlignment": "center"
        }
      },
      "info-card#ic3": {
        "props": {
          "id": "info-card-ic3",
          "isFullModeStyle": true,
           "imageUrl": "assets/fresca.jpg",
           "callToActionText": "COMPRA AQUÍ",
          "callToActionUrl": "/sale/d",
          "blockClass": "info-card",
          "textAlignment": "center"
        }
      },
    
    
      "flex-layout.row#bannerInferior": {
        "props": {
          "verticalAlign": "middle",
          "horizontalAlign": "center"
        },
        "children": ["image#bannerSlim2"]
      },
      "image#bannerSlim2": {  
        "props": {  
          "src": "assets/BannerInferior.jpg"  
        }  
      },
      
      "flex-layout.row#formulario": {
        "props": {
          "verticalAlign": "middle",
          "horizontalAlign": "center"
        },
        "children": ["rich-text#newsletter-text1","newsletter","rich-text#newsletter-text2"]
      },
      "rich-text#newsletter-text1": {
        "props": {
          "text": "Regístrate y sé el primero en recibir noticias de ofertas y promociones especiales",
          "blockClass": "newsletter-izquierdo"
        }
      },
      "rich-text#newsletter-text2": {
        "props": {
          "text": "Al registrar tu correo electrónico aceptas los terminos y condiciones",
          "blockClass": "newsletter-2"
        }
      },
     
      
     
      "rich-text#link": {
        "props": {
          "text": "\n*Sígue Nuestras Redes Sociales*\n",
          "blockClass": "link"
        }
      },
      
      "flex-layout.row#redessociales": {
        "props": {
          "verticalAlign": "middle",
          "horizontalAlign": "center"
        },
        "children": [
          "image#redesfb",
          "rich-text#fb",
          "image#redesig",
          "rich-text#ig"
        ]
      }, 
      
      "image#redesfb": {  
        "props": {  
          "blockClass": "Img-fb",
          "src": "assets/face-pink.png"  
        }  
      },
      "rich-text#fb": {
        "props": {
          "blockClass": "Facebook",
          "text": " @LiliPinkColombia "
        }
      },
      "image#redesig": {  
        "props": { 
          "blockClass": "Img-ig", 
          "src": "assets/insta-pink.png"  
        }  
      },
      "rich-text#ig": {
        "props": {
          "blockClass": "Instagram",
          "text": "@Lilipinkcol"     
        }
      }
    
}