        "blockClass": "info-card-ic2",
        "textAlignment": "center"
      }
    },
    "info-card#ic3Yoi": {
      "props": {
        "id": "info-card-ic3",
        "isFullModeStyle": true,
         "imageUrl": "assets/infoHomeC.jpg",
         "callToActionText": "COMPRA AQUÍ",
        "callToActionUrl": "/sale/d",
        "blockClass": "info-card-ic3",
        "textAlignment": "center"
      }
    },
  
    "flex-layout.row#bannerInferiorYoi": {
      "props": {
        "verticalAlign": "middle",
        "horizontalAlign": "center"
      },
      "children": ["image#bannerSlim2Yoi"]
    },
   
    
    "image#bannerSlim2Yoi": {  
      "props": {  
        "src": "assets/BannerInferior.jpg"  
      }  
    },
  
